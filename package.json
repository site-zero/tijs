{
  "name": "@site0/tijs",
  "version": "1.1.0",
  "description": "Web UI Libraray base on Vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/site-zero/tijs.git"
  },
  "author": "Peter Zhang <zozohtnt@gmail.com>",
  "license": "BSD-3-Clause",
  "private": false,
  "keywords": [
    "ti",
    "tijs",
    "ti-js",
    "vue",
    "vue3",
    "js",
    "ts",
    "ui"
  ],
  "type": "module",
  "main": "./dist/tijs.umd.cjs",
  "module": "./dist/tijs.js",
  "files": [
    "index.d.ts",
    "dist",
    "assets"
  ],
  "exports": {
    ".": {
      "__import": "./dist/tijs.es.js",
      "import": "./index.d.ts",
      "require": "./dist/tijs.umd.js",
      "types": "./index.d.ts"
    },
    "./style.css": "./dist/style.css",
    "./style.scss": "./src/style.scss",
    "./scss": "./src/assets/style/_all.scss"
  },
  "scripts": {
    "build": "vue-tsc && vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "vitest --silent=false",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.16",
    "@types/sprintf-js": "^1.1.3",
    "chance": "^1.1.11",
    "crypto-js": "^4.2.0",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "sprintf-js": "^1.1.3",
    "vue": "^3.4.38",
    "vue-router": "^4.2.5",
    "vue-server-renderer": "^2.7.15"
  },
  "devDependencies": {
    "@types/chance": "^1.1.6",
    "@vitejs/plugin-vue": "^4.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "sass": "^1.70.0",
    "vite": "^5.3.1",
    "vitest": "^0.34.4",
    "vue-tsc": "^2.1.4"
  }
}
